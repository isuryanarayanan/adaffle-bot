/* Imports */
const { SlashCommandBuilder } = require("@discordjs/builders");
const { MessageEmbed } = require("discord.js");
/* Imports */

/* Main */
module.exports = {
  data: new SlashCommandBuilder()
    .setName("random")
    .setDescription("Generates a random number between 1-9")
    .addIntegerOption((options) =>
      options
        .setName("upper_range")
        .setDescription(
          "seperate upper and lower bounds with a comma like 0,10"
        )
        .setRequired(true)
    )
    .addIntegerOption((options) =>
			options.
        .setName("lower_range")
        .setDescription(
          "seperate upper and lower bounds with a comma like 0,10"
        )
        .setRequired(true)
    ),
  async execute(interaction) {
    const input = interaction.options.getString("range");
    console.log(input);
    //await interaction.reply(""+Math.floor((Math.random() * 10) + 1));
  },
};
/* Main */
